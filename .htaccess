## contr√¥le du cache navigateur - Expire headers
<IfModule mod_expires.c>
	ExpiresActive On
	ExpiresDefault "access plus 1 year"
	ExpiresByType img/jpg 			"access plus 1 year"
	ExpiresByType img/jpeg 			"access plus 1 year"
	ExpiresByType img/png 			"access plus 1 year"
	ExpiresByType img/gif 			"access plus 1 year"
	ExpiresByType img/svg+xml			"access plus 1 year"
	ExpiresByType img/bmp             "access plus 1 year"
	AddType image/x-icon .ico
	ExpiresByType img/ico 			"access plus 1 year"
	ExpiresByType img/icon 			"access plus 1 year"
	ExpiresByType img/x-icon 			"access plus 1 year"
	ExpiresByType text/css 				"access plus 1 year"
	ExpiresByType text/javascript 		"access plus 1 year"
	ExpiresByType text/html 			"access plus 1 year"
	ExpiresByType application/xhtml+xml 	"access plus 1 year"
	ExpiresByType application/javascript 	"access plus 1 year"
	ExpiresByType application/x-javascript 	"access plus 1 year"
	ExpiresByType application/x-shockwave-flash "access plus 1 year"
</IfModule>


## cache-control
<IfModule mod_headers.c>

<FilesMatch "\\.(ico|jpe?g|png|gif|swf)$">
Header set Cache-Control "max-age=2592000, public"
</FilesMatch>

<FilesMatch "\\.(min.css)$">
Header set Cache-Control "max-age=2592000, public"
</FilesMatch>

<FilesMatch "\\.(min.js)$">
Header set Cache-Control "max-age=2592000, private"
</FilesMatch>

<FilesMatch "\\.(x?html?|php)$">
Header set Cache-Control "max-age=2592000, private, must-revalidate"
</FilesMatch>

</IfModule>
